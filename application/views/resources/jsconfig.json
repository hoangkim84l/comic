{
    "compilerOptions": {
        "target": "es5",
        "allowJs": true,
        "strict": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "baseUrl": "./",
        "paths": {
            "~/*": [
                "./src/*"
            ]
        }
    }
}
